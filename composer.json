{
  "name": "phalcon/website",
  "description": "Official Phalcon Website",
  "keywords": [
    "website",
    "phalcon",
    "framework",
    "extension"
  ],
  "type": "project",
  "license": "BSD-3-Clause",
  "homepage": "https://phalconphp.com/",
  "authors": [
    {
      "name": "Phalcon Team",
      "email": "team@phalconphp.com",
      "homepage": "https://phalconphp.com/en/team"
    },
    {
      "name": "Contributors",
      "homepage": "https://github.com/phalcon/cphalcon/graphs/contributors"
    }
  ],
  "support": {
    "source": "https://github.com/phalcon/cphalcon",
    "issues": "https://github.com/phalcon/cphalcon/issues",
    "wiki": "https://github.com/phalcon/cphalcon/wiki",
    "forum": "https://forum.phalconphp.com/",
    "docs": "https://docs.phalconphp.com"
  },
  "autoload": {
    "psr-4": {
      "Website\\": "app/library/",
      "Website\\Cli\\Tasks\\": "app/tasks/",
      "Website\\Controllers\\": "app/controllers/",
      "Website\\Middleware\\": "app/library/Middleware/"
    }
  },
  "require": {
    "php": ">= 7.2",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "vlucas/phpdotenv": "^2.4",
    "guzzlehttp/guzzle": "~6.0",
    "swiftmailer/swiftmailer": "^5.4",
    "dariuszp/cli-progress-bar": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "codeception/codeception": "^2.2",
    "codeception/verify": "~0.3",
    "codeception/specify": "~0.4",
    "codeception/mockery-module": "^0.2",
    "squizlabs/php_codesniffer": "~2.7"
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  }
}
